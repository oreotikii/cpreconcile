{
  "name": "cpreconcile",
  "version": "1.0.0",
  "description": "Backend reconciliation system for Shopify, Razorpay, and Easyecom",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "nodemon --exec ts-node src/index.ts",
    "test:easyecom": "ts-node scripts/test-easyecom-auth.ts",
    "test:easyecom:login": "ts-node scripts/test-easyecom-login.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "lint": "eslint src --ext .ts",
    "test": "jest"
  },
  "keywords": [
    "reconciliation",
    "shopify",
    "razorpay",
    "easyecom",
    "payment-gateway"
  ],
  "author": "Aritra Ghosh",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "axios": "^1.6.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.7",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.8.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
